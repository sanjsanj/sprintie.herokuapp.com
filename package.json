{
  "name": "sprintie.herokuapp.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint": "^4.18.1",
    "node-sass-chokidar": "0.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^5.0.7",
    "react-scripts": "5.0.1",
    "redux": "^3.7.2",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "e2e-tests": "cypress open",
    "scss": "node-sass-chokidar src/styles/ -o src/styles/",
    "scss:watch": "npm run scss && node-sass-chokidar src/styles/ -o src/styles/ --watch --recursive",
    "start": "cross-env PORT=3010 react-scripts start",
    "build": "npm run scss && react-scripts build",
    "build:watch": "node src/watch.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "cypress": "^2.1.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0"
  }
}
